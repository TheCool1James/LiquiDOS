{
    "files.associations": {
        "*.h": "cpp",
        "typeinfo": "c"
    },
    
    "psi-header.changes-tracking": {
		"isActive": true,
		"modAuthor": "@date Last modified by:   ",
		"modDate": "@date Last modified time: ",
		"modDateFormat": "YYYY-MM-DDTHH:mm:ssZ",
		"autoHeader": "manualSave"
    },
    
    "psi-header.config": {},
    "psi-header.lang-config": [
        {
			"language": "c",
			"begin": "/**",
			"prefix": " * ",
			"end": " */",
			"blankLinesAfter": 0,
			"forceToTop": false
        },
        {
			"language": "cpp",
			"begin": "/**",
			"prefix": " * ",
			"end": " */",
			"blankLinesAfter": 0,
			"forceToTop": false
		},
    ],
    "psi-header.license-text": [],

    "psi-header.variables": [
        ["authoremail", "kevindai02@outlook.com"],
        ["author", "Kevin Dai"]
    ],

    "psi-header.templates": [{
        "language": "cpp",
        "template": [
            "@file   <<filename>>",
            "@author <<author>> \\<<<authoremail>>\\>",
            "",
            "@date Created on <<filecreated('dddd, MMMM Do YYYY, h:mm:ss a')>>",
            "",
            "@date Last modified by:   ",
            "@date Last modified time: "
        ]
    },
    {
        "language": "c",
        "template": [
            "File:   <<filename>>",
            "Author: <<author>>",
            "Email:  <<authoremail>>",
            "",
            "@date Created on <<filecreated('dddd, MMMM Do YYYY, h:mm:ss a')>>",
            "",
            "@date Last modified by:   ",
            "@date Last modified time: "
        ]
    }
],
"doxdocgen.generic.order": [
    "empty",
    "tparam",
    "param",
    "return"
],
}