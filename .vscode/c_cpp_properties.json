{
    "configurations": [
        {
            "name": "Win32",
            "browse": {
                "path": [
                    "${workspaceFolder}/**"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "includePath": [
                "${workspaceFolder}/include/",
                "${workspaceFolder}/lib/libc/include/",
                "C:/cygwin/lib/gcc/i686-pc-cygwin/7.3.0/include",
                "C:/cygwin/lib/gcc/i686-pc-cygwin/7.3.0/include"
            ],
            "cStandard": "c99",
            "cppStandard": "c++11",
            "intelliSenseMode": "clang-x64",
            "compilerPath": "\"C:/cygwin/bin/g++.exe\" -std=c++14 -Iinclude -nostdinc++ -nostdinc -ffreestanding -fno-rtti -fno-exceptions"
        }
    ],
    "version": 4
}