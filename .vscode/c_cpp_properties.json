{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/include/**",
                "${workspaceFolder}/build/",
                "/usr/include/x86_64-linux-gnu/c++/7"
            ],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c99",
            "cppStandard": "c++11",
            "intelliSenseMode": "gcc-x86",
            "compileCommands": "build/cmake-build/compile_commands.json",
            "compilerArgs": [],
            "browse": {
                "path": [
                    "${workspaceFolder}/**",
                    "${workspaceFolder}/include/**",
                    "${workspaceFolder}/build/",
                    "/usr/include/x86_64-linux-gnu/c++/7"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        },
        {
            "name": "WSL",
            "includePath": [
                "${workspaceFolder}/include/**",
                "${workspaceFolder}/build",
                "${workspaceFolder}/lib/elf/include",
                "${workspaceFolder}/lib/libc/include",
                "${workspaceFolder}/lib/ebl/include",
                "${workspaceFolder}/lib/openlibm/include",
                "${workspaceFolder}/lib/openlibm/src"
            ],
            "defines": [
                "__KERNEL__",
                "__FILENAME_CUSTOM__=\"\""
            ],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c99",
            "cppStandard": "c++11",
            "intelliSenseMode": "gcc-x86",
            "compilerArgs": [
                "-ffreestanding",
                "-std=c++14",
                "-fno-rtti",
                "-fno-exceptions",
                "-nostdinc++"
            ]
        }
    ],
    "version": 4
}